<main class="container container-fluid mt-5">
        <section class="previewCarousel">

            <div id="carouselControls" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img src="./Public/images/liffchaser-carousel-1.jpg" class="d-block w-100" alt="learnCode">
                  </div>
                  <div class="carousel-item">
                    <img src="./Public/images/liffchaser-carousel-2.jpg" class="d-block w-100" alt="learnCollab">
                  </div>
                  <div class="carousel-item">
                    <img src="./Public/images/liffchaser-carousel-3.jpg" class="d-block w-100" alt="learnRead">
                  </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="visually-hidden">Next</span>
                </button>
              </div>

        </section>

        <section id="signUpForm">

            <form class="signUpForm">

                <div class="mb-3">
                    <label for="InputEmail" class="form-label">Email</label>
                    <input type="email" class="form-control" id="exampleInputEmail1">
                </div>

                <div class="mb-3">
                    <label for="InputPassword" class="form-label">Password</label>
                    <input type="password" class="form-control" id="InputPassword">
                </div>

                <div id="signUpBtn"><button type="submit" class="btn btn-info">Sign Up</button></div>

                <p id="or">or</p>

                <div id="logInBtn"><button type="submit" class="btn btn-info">Log In</button></div>

            </form>
        </section>

        <section class="logInForm">
            <form>
                <div class="mb-3">
                    <label for="InputEmail" class="form-label">Email</label>
                    <input type="email" class="form-control" id="InputEmail2">
                </div>

                <div class="mb-3">
                    <label for="InputPassword" class="form-label">Password</label>
                    <input type="password" class="form-control" id="InputPassword2">
                </div>

                <div id="logInBtn"><button type="submit" class="btn btn-info">Log In</button></div>
            </form>
        </section>

    </main>
